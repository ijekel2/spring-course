package com.nathanjekel.spring.springcore;

import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Test {
	
	public static void main(String[] args) {
		
		// Searches for XML config file on the ClassPath
		ClassPathXmlApplicationContext ctx = new ClassPathXmlApplicationContext("com/nathanjekel/spring/springcore/config.xml");
		Employee emp = (Employee) ctx.getBean("emp");
		System.out.println("Employee ID: " + emp.getId());
		System.out.println("Employee Name: " + emp.getName());
	}

}
